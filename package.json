{
  "name": "@caas/upload-artifact",
  "version": "4.0.0",
  "description": "Upload a build artifact that can be used by subsequent workflow steps",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "release": "ncc build src/upload-or-download.ts && git add -f dist/",
    "check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:test\" \"npm:build\"",
    "fix": "prettier -w . && pnpm eslint --quiet --fix .",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "test": "jest --testTimeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/upload-artifact.git"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Artifacts",
    "Upload"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actions/upload-artifact/issues"
  },
  "homepage": "https://github.com/actions/upload-artifact#readme",
  "dependencies": {
    "@actions/artifact": "^1.1.2",
    "@actions/core": "^1.10.0",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2",
    "@aws-sdk/client-s3": "3.431.0",
    "@aws-sdk/signature-v4-crt": "3.431.0",
    "@aws-sdk/types": "3.428.0",
    "p-map": "^4.0.0",
    "pmap": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/parser": "^6.9.1",
    "@vercel/ncc": "^0.38.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
